  "scripts": {
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "dotenv -e .env -- tsx server/index.ts",
    "dev:client": "vite --force",
    "test": "echo \"Error: no test specified\" && exit 1",
    "import:sp500": "tsx server/sp500-scanner.ts",
    "enhance:returns": "tsx server/returns-enhancer.ts",
    "enhance:peg": "tsx server/peg-ratio-enhancer.ts",
    "enhance:drawdown": "tsx server/drawdown-enhancer.ts",
    "run:scan": "tsx server/run-sp500-scan.ts",
    "run:fix-missing": "tsx server/fix-missing-companies.ts",
    "enhance:all": "npm-run-all -s enhance:returns enhance:drawdown enhance:armdd enhance:revenue-growth enhance:fcf enhance:calculated-metrics",
    "enhance:all-but-peg": "npm run enhance:returns && npm run enhance:drawdown",
    "enhance:armdd": "tsx server/ar-mdd-enhancer.ts",
    "enhance:calculated-metrics": "tsx server/calculated-metrics-enhancer.ts",
    "enhance:fcf": "tsx server/fcf-enhancer.ts",
    "enhance:revenue-growth": "tsx server/revenue-growth-enhancer.ts",
