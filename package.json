    "import:nasdaq": "tsx server/nasdaq100-import.ts",
    "import:dowjones": "tsx server/dow-jones-import.ts",
    "enhance:nasdaq:returns": "tsx server/nasdaq100-returns-enhancer.ts",
    "enhance:nasdaq:drawdown": "tsx server/nasdaq100-drawdown-enhancer.ts",
    "enhance:nasdaq:armdd": "tsx server/nasdaq100-ar-mdd-enhancer.ts",
    "debug:returns": "tsx server/debug-returns.ts",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push:pg",
    "db:studio": "drizzle-kit studio",
    "start:prod": "node dist/index.js",
    "test:db": "tsx server/test-db-connection.ts",
    "db:migrate": "dotenv -e .env -- drizzle-kit migrate",
    "db:drop": "tsx server/drop-tables.ts",
    "db:clean": "tsx server/clean-data.ts",
    "db:verify": "tsx server/verify-data.ts",
    "db:fix-missing": "tsx server/fix-missing-companies.ts",
    "fix:sp500-pe": "tsx server/fix-sp500-pe.ts",
    "preview": "vite preview",
    "verify:sp500-count": "tsx server/verify-sp500-count.ts",
    "update:daily": "tsx server/daily-updater.ts",
    "populate:base": "tsx server/populate-base-metrics.ts",
    "import:sp500-csv": "tsx server/import-sp500-from-csv.ts"
